{
  "name": "ustazia",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://zebington.github.io/ustazia",
  "dependencies": {
    "apollo-client": "^1.9.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-dom": "^15.6.1",
    "react-image-lightbox": "^4.2.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13",
    "react-transition-group": "^1.2.0"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src src/App.scss src/App.css",
    "watch-css": "npm run build-css && node-sass --include-path ./src src/App.scss src/App.css --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build&&gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^1.0.0"
  }
}
